        "copyfiles": "^2.4.1",
        "html-loader": "^5.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.4",
        "terser-webpack-plugin": "^5.3.10",
        "ts-node": "^10.9.2",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0",
        "webpack-obfuscator": "^3.5.1"
    }
